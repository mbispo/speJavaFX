CREATE TABLE LOG (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, ENVIADO SMALLINT DEFAULT 0, EMPRESAADMINISTRADOR INTEGER, mensagem VARCHAR(6000), MATRICULAADMINISTRADOR INTEGER, CLASSE VARCHAR(255), TIPOOPERACAO INTEGER, DATAHORA TIMESTAMP, CODIGOREDUZIDOORGANOGRAMA INTEGER, METODO VARCHAR(255), CODIGOATO INTEGER, TIPO INTEGER, FUNCIONARIO_Cd_Fnc INTEGER, PRIMARY KEY (ID))
CREATE TABLE PONTO (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, Cd_LclMnc INTEGER NOT NULL, stc_pnt INTEGER NOT NULL, Dt_Pnt TIMESTAMP NOT NULL, Cd_Fnc INTEGER NOT NULL, PRIMARY KEY (ID))
CREATE TABLE FUNCIONARIO (Cd_Fnc INTEGER NOT NULL, IsentaDigital SMALLINT DEFAULT 0 NOT NULL, SenhaIntranet VARCHAR(255) NOT NULL, Nm_Fnc VARCHAR(255) NOT NULL, Lotacao VARCHAR(255) NOT NULL, PRIMARY KEY (Cd_Fnc))
