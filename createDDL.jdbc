CREATE TABLE PARAMETRO (ID INTEGER NOT NULL, DS_IntEnvRgsOpr VARCHAR(255) NOT NULL, NR_NvlTlrVrf INTEGER NOT NULL, DS_IntEnvDgt VARCHAR(255) NOT NULL, DS_IntScrRlg VARCHAR(255) NOT NULL, DS_IntEnvRgsPnt VARCHAR(255) NOT NULL, NR_DiaEnvDgt INTEGER NOT NULL, DS_IntRcbDgt VARCHAR(255) NOT NULL, DS_SnhMst VARCHAR(255) NOT NULL, versaoBD VARCHAR(255) NOT NULL, PRIMARY KEY (ID))
